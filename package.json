{
    "name": "tubegpt-clean",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build && tsc --project tsconfig.worker.json",
        "worker:build": "tsc --project tsconfig.worker.json",
        "start": "next start",
        "lint": "next lint",
        "clean": "rimraf .next dist",
        "worker": "tsx src/worker/extract.ts",
        "worker:prod": "node dist/worker/extract.js",
        "worker:dev": "node --watch --loader=tsx src/worker/extract.ts",
        "test-worker": "node test-worker.js",
        "direct-test": "node direct-worker-test.js",
        "test-extract": "cross-env NODE_ENV=development DATABASE_URL=postgresql://test:test@test.neon.tech/test NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test CLERK_SECRET_KEY=sk_test OPENAI_API_KEY=sk_test LEAPCELL=true DEPLOYMENT_ENV=leapcell tsx src/worker/extract.ts",
        "start-app": "node start-app.js",
        "start-app:prod": "NODE_ENV=production node start-app.js",
        "fix-env": "node fix-env.js",
        "fix-redis": "node fix-redis.js",
        "setup": "npm run fix-env && npm run fix-redis",
        "dev-all": "node start-dev.js",
        "dev-worker": "node dev-worker.js",
        "prod-test": "node start-prod.js",
        "prod-worker": "bash start-worker.sh",
        "start-prod": "WORKER_ENABLED=true node start-prod.js",
        "start-dev-complete": "npm run fix-env && npm run dev-all",
        "test-payment-webhook": "node test-payment-webhook.js",
        "test-subscription": "node test-subscription.js",
        "test-subscription-simple": "node test-subscription-simple.js",
        "setup-redis": "node setup-redis.js",
        "test-redis": "node setup-redis.js",
        "worker-redis": "node start-worker-redis.js"
    },
    "dependencies": {
