        "@types/react-dom": "^18.3.7",
        "@upstash/ratelimit": "^1.2.1",
        "@upstash/redis": "^1.35.0",
        "axios": "^1.10.0",
        "axios-retry": "^4.5.0",
        "bullmq": "^4.18.2",
        "class-variance-authority": "0.7.1",
        "clsx": "^2.1.1",
        "dotenv": "^16.5.0",
        "embla-carousel-react": "^8.6.0",
        "lucide-react": "^0.511.0",
        "next": "^14.2.3",
        "node-fetch": "^3.3.2",
        "openai": "^5.3.0",
        "postcss": "^8.4.40",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.57.0",
        "react-markdown": "^10.1.0",
        "sonner": "^2.0.5",
